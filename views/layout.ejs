  <!-- Primary Meta Tags -->
  <title><%= title || 'FurniCraft - Premium Handcrafted Furniture' %></title>
  <meta name="title" content="<%= title || 'FurniCraft - Premium Handcrafted Furniture' %>">
  <meta name="description" content="<%= typeof description !== 'undefined' ? description : 'Discover handcrafted, sustainable furniture for modern living. Free shipping on all orders over $499.' %>">
  <meta name="keywords" content="<%= typeof keywords !== 'undefined' ? keywords : 'furniture, handmade furniture, sustainable furniture, modern furniture, home decor' %>">
  <meta name="author" content="FurniCraft">
  <meta name="robots" content="index, follow">

  <!-- Open Graph / Facebook (Social Media Sharing) -->
  <meta property="og:type" content="website">
  <meta property="og:url" content="https://furnicraft.com/">
  <meta property="og:title" content="<%= title || 'FurniCraft - Premium Handcrafted Furniture' %>">
  <meta property="og:description" content="<%= typeof description !== 'undefined' ? description : 'Discover handcrafted, sustainable furniture for modern living.' %>">
  <meta property="og:image" content="<%= typeof image !== 'undefined' ? image : '/img/social-share.jpg' %>">

  <!-- Twitter -->
  <meta property="twitter:card" content="summary_large_image">
  <meta property="twitter:url" content="https://furnicraft.com/">
  <meta property="twitter:title" content="<%= title || 'FurniCraft - Premium Handcrafted Furniture' %>">
  <meta property="twitter:description" content="<%= typeof description !== 'undefined' ? description : 'Discover handcrafted, sustainable furniture for modern living.' %>">
  <meta property="twitter:image" content="<%= typeof image !== 'undefined' ? image : '/img/social-share.jpg' %>">

  <!-- Favicons -->
  <link rel="apple-touch-icon" sizes="128x128" href="/img/logo-128.png">
  <link rel="icon" type="image/png" sizes="64x64" href="/img/logo-64.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/img/logo-32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/img/logo-16.png">
  <link rel="manifest" href="/site.webmanifest">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">

  <!-- CSS -->
  <link rel="stylesheet" href="/css/style.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
  
  <!-- Canonical URL (SEO) -->
  <link rel="canonical" href="https://furnicraft.com<%= originalUrl || url %>">

  <!-- Mobile Meta -->
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Poppins:400,500,600,700" rel="stylesheet">

  <link rel="stylesheet" href="/libs/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" href="/libs/font-awesome/css/font-awesome.min.css">
  <link rel="stylesheet" href="/libs/font-material/css/material-design-iconic-font.min.css">
  <link rel="stylesheet" href="/libs/nivo-slider/css/nivo-slider.css">
  <link rel="stylesheet" href="/libs/nivo-slider/css/animate.css">
  <link rel="stylesheet" href="/libs/nivo-slider/css/style.css">
  <link rel="stylesheet" href="/libs/owl-carousel/assets/owl.carousel.min.css">
  <link rel="stylesheet"  type="text/css" href="/css/style.css"/>
  <link rel="stylesheet" type="text/css" href="/css/reponsive.css">
  <link rel="stylesheet"  type="text/css" href="/css/main.css"/>
  <link rel="stylesheet" href="/css/mobiscroll.jquery.min.css">


  <script src="https://code.jquery.com/jquery-1.11.2.min.js"></script>
  <script src="/js/mobiscroll.jquery.min.js"></script>
  <script>
    mobiscroll.setOptions({
      locale: mobiscroll.localeEn,
      theme: "ios",
      themeVariant: "light",
    });
  
    var popup;
  
    $(function () {
      popup = $("#demo-popup")
        .mobiscroll()
        .popup({
          display: window.innerWidth < 768 ? "bottom" : "center",
        })
        .mobiscroll("getInst");
  
      $("#demo-popup-close").on("click", function () {
        popup.close();
      });
    });
  
    function showPopup(type, title, message, next = { link: null, text: "" }) {
        $("#popup-title").text(title);
        $("#popup-message").text(message);
      
        if (type === "success") {
          $("#popup-icon").addClass("success-icon").removeClass("error-icon");
        } else {
          $("#popup-icon").addClass("error-icon").removeClass("success-icon");
        }
      
        const popupButton = $("#popup-action-btn");
        const closeButton = $("#demo-popup-close");
      
        if (next.link) {
          popupButton.text(next.text).show().off("click").on("click", function () {
            window.location.href = next.link;
          });
          closeButton.hide();
        } else {
          popupButton.hide();
          closeButton.show();
          setTimeout(() => {
            popup.close();
          }, 3000);
        }
      
        popup.open();
      }
      
  </script>
  
</head>
